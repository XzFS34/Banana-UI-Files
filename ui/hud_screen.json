  "namespace": "hud",

  // Button Images //

  "buang_1_item": {
    "type": "image",
    "size": [ 18, 18 ],
    "texture": "textures/ui-x/icon-new/drop_1x"
  },

  "buang_stack_item": {
    "type": "image",
    "size": [ 18, 18 ],
    "texture": "textures/ui-x/icon-new/drop_64x"
  },

