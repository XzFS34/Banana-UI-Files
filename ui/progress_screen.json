            {
              "panel_loading": { "type": "panel","size": [ 15, "100%" ],"controls": [{ "loading_spin@progress.loading_spin": {"size": [ "100%" , "100%" ]}}]}},
            {
              "spacer_b": { "type": "panel", "size": [ 15, 15 ]}
            },
            {
              "judul_stack_2": {
                "size": [ "fill", "100%" ],
                "type": "panel",
                "controls": [
                  {
                    "judul_stack_uwu": {
                      "type": "stack_panel",
                      "controls": [
                        {
                          "label_panel": { "type": "panel", "size": [ "100%", "100%c" ], "controls": [{"judul@progress.custom_label_progress": {"anchor_from": "top_left", "anchor_to": "top_left", "size": [ "default", "default" ],"text": "#title_text","bindings": [{"binding_name": "#title_text"}]}}]}
                        },
                        {
                          "spacer_2": { "type": "panel", "size": [ 1.5, 1.5 ]}
                        },
                        {
                          "label_panel_2": { "type": "panel", "size": [ "100%", "100%c" ], "controls": [{"deskripsi@progress.custom_label_progress": {"anchor_from": "top_left", "anchor_to": "top_left", "size": [ "default", "default" ],"text": "#progress_text","bindings": [{"binding_name": "#progress_text"}]}}]}
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

