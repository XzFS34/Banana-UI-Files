  "modal_progress_screen@progress_screen": {
    "$screen_content": "progress.modal_screen_content"
  },
  
  "progress_title_text": {
    "type": "panel",
    "anchor_from": "center",
    "anchor_to": "center",
    "size": [ 200, "100%c" ],
    "controls": [
      {
        "progress_title": {
          "type": "stack_panel",
          "controls": [
            {
              "loading_spin@progress.loading_spin": {
                "anchor_from": "center",
                "anchor_to": "center",
                "size": [ "100%" , 15 ]
              }
            },
            { "spacer": { "type": "panel", "size": [ 15, 15 ]}},
            {
              "panel_label": {
                "type": "panel",
                "size": [ "100%", "100%c" ],
                "controls": [
                  {
                    "progress_title_text@progress.progress_screen_text": {
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "text": "#title_text",
                      "bindings": [
                        {
                          "binding_name": "#title_text"
                        }
                      ]
                    }
                  }
                ]
              }
            },
            { "spacer_2": { "type": "panel", "size": [ 7, 7 ]}},
            {
              "panel_label_2": {
                "type": "panel",
                "size": [ "100%", "100%c" ],
                "controls": [
                  {
                    "progress_bar_text@progress.progress_screen_text": {
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "text": "#progress_text",
                      "bindings": [
                        { "binding_name": "#progress_text" }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },


  "custom_label_progress": {
    "type": "label",
    "font_type": "PoppinsMedium",
    "font_scale_factor": 1,
    "size": [ "100%", "default"],
    "text_alignment": "left",
    "layer": 2,
    "shadow": false
  },


  "judul_baru_new@neko_c.bg_3": {
