    "anchor_to": "center",
    "$border_visible": false,
    "$default_button_texture|default": "textures/neko/borderless/default",
    "$hover_button_texture|default": "textures/neko/borderless/hover",
    "$pressed_button_texture|default": "textures/neko/borderless/pressed",
