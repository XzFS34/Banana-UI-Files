  },

  "pause_button_more_button@common_buttons.neko_button_uwu": {
    "$button_type_panel": "play_ui.custom_button_template",
    "$default_button_texture": "textures/neko/borderless/default",
    "$hover_button_texture": "textures/neko/borderless/hover",
    "$pressed_button_texture": "textures/neko/borderless/pressed",
    "$locked_button_texture": "textures/neko/borderless/pressed_no_hover",
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$border_visible": true,
    "size": [ "100% - 3px" , "100% - 3px" ],
